datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id               Int      @id @default(autoincrement())
  username         String
  email            String   @unique
  password         String
  name             String
  age              Int
  gender           String
  location         String
  interest         String
  profile_picture  String

}



model Annonces {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  image_url   String
  link_url    String
  created_at  DateTime
}
